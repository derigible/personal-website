    "@instructure/ui-layout": "^5.36.0",
    "@instructure/ui-presets": "^5.36.0",
    "@instructure/ui-themeable": "^5.36.0",
    "@instructure/ui-themes": "^5.36.0",
    "@instructure/ui-utils": "^5.36.0",
    "classnames": "^2",
    "date-fns": "^1.29.0",
